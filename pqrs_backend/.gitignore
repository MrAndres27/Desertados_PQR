cat > .gitignore << 'EOF'
# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
build/
dist/
*.egg-info/
venv/
env/

# IDE
.vscode/
.idea/
*.swp

# Environment
.env
.env.local

# Database
*.db
*.sqlite

# Logs
logs/
*.log

# Uploads
uploads/*
!uploads/.gitkeep

# Exports
exports/*
!exports/.gitkeep

# Testing
.pytest_cache/
.coverage
htmlcov/

# OS
.DS_Store
Thumbs.db